<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="524.0" prefWidth="453.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.Add_PartController">
   <children>
      <Label layoutX="14.0" layoutY="21.0" text="Add Part" textFill="#3716dd">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ButtonBar layoutX="127.0" layoutY="14.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <RadioButton fx:id="InHouseRadioButton" mnemonicParsing="false" onAction="#updateLabelText" text="In-House">
               <toggleGroup>
                  <ToggleGroup fx:id="ToggleGroup1" />
               </toggleGroup></RadioButton>
            <RadioButton fx:id="OutsourcedRadioButton" mnemonicParsing="false" onAction="#updateLabelText" prefHeight="23.0" prefWidth="129.0" text="Outsourced" toggleGroup="$ToggleGroup1" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="IdTextField" disable="true" layoutX="153.0" layoutY="87.0" prefHeight="40.0" prefWidth="149.0" text="Auto Gen - Disabled" />
      <TextField fx:id="Name" layoutX="152.0" layoutY="146.0" prefHeight="40.0" prefWidth="149.0" text="Part Name" />
      <TextField fx:id="Inventory" layoutX="153.0" layoutY="202.0" prefHeight="40.0" prefWidth="149.0" text="Inv" />
      <TextField fx:id="Price" layoutX="153.0" layoutY="259.0" prefHeight="40.0" prefWidth="149.0" text="Price/Cost" />
      <TextField fx:id="Max" layoutX="153.0" layoutY="335.0" prefHeight="40.0" prefWidth="85.0" text="Max" />
      <TextField fx:id="Min" layoutX="302.0" layoutY="335.0" prefHeight="40.0" prefWidth="85.0" text="Min" />
      <Label layoutX="35.0" layoutY="99.0" text="ID" />
      <Label layoutX="35.0" layoutY="158.0" text="Name" />
      <Label layoutX="36.0" layoutY="214.0" text="Inv" />
      <Label layoutX="35.0" layoutY="271.0" text="Price/Cost" />
      <Label layoutX="38.0" layoutY="347.0" text="Max" />
      <Label layoutX="255.0" layoutY="347.0" text="Min" />
      <TextField fx:id="CompanyName" layoutX="153.0" layoutY="395.0" prefHeight="40.0" prefWidth="99.0" text="Comp Num" />
      <Label fx:id="CompanyNameLabel" layoutX="37.0" layoutY="407.0" text="Company Name" />
      <ButtonBar layoutX="227.0" layoutY="461.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="SaveButton" mnemonicParsing="false" onAction="#saveNewPart" text="Save" />
            <Button fx:id="CancelButton" mnemonicParsing="false" onAction="#reloadMainScreen" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
